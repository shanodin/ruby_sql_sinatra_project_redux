<h1>Pet Owners</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Phone Number</th>
    <th>Details</th>
  </tr>
<% for owner in @owners %>
  <tr>
    <td><%= owner.name %></td>
    <td><%= owner.phone_number %></td>
    <td><a href="/owners/<%= owner.id %>">Details</a></td>
  </tr>
<% end %>
</table>

<div class="button"><a href="/owners/new">Add a new owner</a></div>
